{"ast":null,"code":"import { delete_all, delete_task, get_task, new_task, update_task } from '@/services/api';\nimport InputGroup1 from './input-group-1/InputGroup1.vue';\nimport TodoItems from './todoItems/TodoItems.vue';\nexport default {\n  name: 'BaseComponent',\n  components: {\n    InputGroup1,\n    TodoItems\n  },\n  data() {\n    return {\n      tasks: []\n    };\n  },\n  mounted() {\n    get_task().then(response => {\n      this.tasks = response.data[\"data\"];\n    }).catch(error => {\n      console.log(\"Error fetching tasks\", error);\n    });\n  },\n  methods: {\n    createTask(task) {\n      const data = {\n        task: task,\n        done: false\n      };\n      new_task(data).then(response => {\n        console.log(\"msg\", response.data);\n        this.getTask();\n      }).catch(error => {\n        console.log(\"error\", error);\n      });\n    },\n    getTask() {\n      get_task().then(response => {\n        this.tasks = response.data[\"data\"];\n      }).catch(error => {\n        console.log(\"Error fetching tasks\", error);\n      });\n    },\n    updateTask(obj) {\n      update_task(obj.id, obj.update).then(response => {\n        console.log(\"msg\", response.data);\n        this.getTask();\n      }).catch(error => {\n        console.log(\"Erro ao atualizar task\", error);\n        this.getTask();\n      });\n    },\n    deleteTask(id) {\n      delete_task(id).then(response => {\n        console.log(\"msg\", response.data);\n        this.getTask();\n      }).catch(error => {\n        console.log(\"error\", error);\n      });\n    },\n    deleteAll() {\n      delete_all().then(response => {\n        console.log(\"msg\", response.data);\n      }).catch(error => {\n        console.log(\"error\", error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["delete_all","delete_task","get_task","new_task","update_task","InputGroup1","TodoItems","name","components","data","tasks","mounted","then","response","catch","error","console","log","methods","createTask","task","done","getTask","updateTask","obj","id","update","deleteTask","deleteAll"],"sources":["/home/eduardo/Workspace/vue-todo-3/frontend/src/components/BaseComponent.vue"],"sourcesContent":["<template>\n  <!-- @keyup.ctrl.delete=\"tasks=[]\" -->\n    <div class=\"col-12\" >\n      <div class=\"card col-10 col-lg-3 col-md-6 col-sm-6\">\n        <div class=\"card-header\">\n          <h3><strong>TO-DO LIST</strong></h3>\n        </div>\n        <InputGroup1 @create-task=\"createTask\" />\n        <div class=\"card-body\">\n          <ul>\n          <TodoItems :items=\"tasks\" @delete-task=\"deleteTask\" @update-task=\"updateTask\"/>\n          \n          </ul>\n  \n        </div>\n       \n        <div class=\"card-footer\">\n          <div class=\"footer-content\">\n            <div class=\"itens-count\">\n              <a href=\"#\"> Tasks: {{ tasks.filter(i => i.done == false).length  }}</a>\n  \n            </div>\n            <div class=\"delete-itens\">\n              <a href=\"#\" @click=\"deleteAll\"><i class=\"bi bi-trash3-fill\"></i></a>\n  \n            </div>\n            \n          </div>\n          \n        </div>\n        \n    </div>\n  \n    </div>\n  \n  </template>\n  \n  <script>\nimport { delete_all, delete_task, get_task, new_task, update_task } from '@/services/api';\nimport InputGroup1 from './input-group-1/InputGroup1.vue';\nimport TodoItems from './todoItems/TodoItems.vue';\n  \n  \n  export default {\n    name: 'BaseComponent',\n    components: {\n      InputGroup1,\n      TodoItems,\n    },\n    data(){\n      return{\n        tasks: []\n      }\n    }, \n    mounted(){\n      get_task()\n        .then((response) =>{\n          this.tasks = response.data[\"data\"];\n        })\n        .catch((error) =>{\n          console.log(\"Error fetching tasks\", error);\n        });\n    },\n    methods:{\n      createTask(task){\n       const data = {task: task, done:false};\n        new_task(data)\n          .then((response) =>{\n            console.log(\"msg\", response.data);\n            this.getTask();\n          }).catch((error) =>{\n            console.log(\"error\",error);\n          });\n\n      },\n      getTask(){\n        get_task()\n        .then((response) =>{\n          this.tasks = response.data[\"data\"];\n        })\n        .catch((error) =>{\n          console.log(\"Error fetching tasks\", error);\n        });\n      },\n      updateTask(obj){\n        update_task(obj.id, obj.update)\n          .then((response) => {\n            console.log(\"msg\", response.data)\n            this.getTask();\n          }).catch((error) =>{\n            console.log(\"Erro ao atualizar task\",error);\n            this.getTask();\n          });\n      },\n      deleteTask(id){\n        delete_task(id)\n          .then((response) =>{\n            console.log(\"msg\", response.data);\n            this.getTask();\n          }).catch((error) =>{\n            console.log(\"error\", error);\n          });\n      },\n      deleteAll(){\n        delete_all()\n          .then((response) =>{\n            console.log(\"msg\", response.data);\n          }).catch((error) => {\n            console.log(\"error\", error);\n          })\n\n      }\n     \n    }\n  }\n  </script>\n  "],"mappings":"AAsCA,SAASA,UAAU,EAAEC,WAAW,EAAEC,QAAQ,EAAEC,QAAQ,EAAEC,WAAU,QAAS,gBAAgB;AACzF,OAAOC,WAAU,MAAO,iCAAiC;AACzD,OAAOC,SAAQ,MAAO,2BAA2B;AAG/C,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVH,WAAW;IACXC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,KAAK,EAAE;IACT;EACF,CAAC;EACDC,OAAOA,CAAA,EAAE;IACPT,QAAQ,CAAC,EACNU,IAAI,CAAEC,QAAQ,IAAI;MACjB,IAAI,CAACH,KAAI,GAAIG,QAAQ,CAACJ,IAAI,CAAC,MAAM,CAAC;IACpC,CAAC,EACAK,KAAK,CAAEC,KAAK,IAAI;MACfC,OAAO,CAACC,GAAG,CAAC,sBAAsB,EAAEF,KAAK,CAAC;IAC5C,CAAC,CAAC;EACN,CAAC;EACDG,OAAO,EAAC;IACNC,UAAUA,CAACC,IAAI,EAAC;MACf,MAAMX,IAAG,GAAI;QAACW,IAAI,EAAEA,IAAI;QAAEC,IAAI,EAAC;MAAK,CAAC;MACpClB,QAAQ,CAACM,IAAI,EACVG,IAAI,CAAEC,QAAQ,IAAI;QACjBG,OAAO,CAACC,GAAG,CAAC,KAAK,EAAEJ,QAAQ,CAACJ,I