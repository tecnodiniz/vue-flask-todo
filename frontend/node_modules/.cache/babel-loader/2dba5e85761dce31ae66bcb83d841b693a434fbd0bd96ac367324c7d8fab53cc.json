{"ast":null,"code":"import { get_task, update_task } from '@/services/api';\nimport InputGroup1 from './input-group-1/InputGroup1.vue';\nimport TodoItems from './todoItems/TodoItems.vue';\nexport default {\n  name: 'BaseComponent',\n  components: {\n    InputGroup1,\n    TodoItems\n  },\n  data() {\n    return {\n      tasks: []\n    };\n  },\n  mounted() {\n    get_task().then(response => {\n      this.tasks = response.data[\"data\"];\n    }).catch(error => {\n      console.log(\"Error fetching tasks\", error);\n    });\n  },\n  methods: {\n    getTask() {\n      get_task().then(response => {\n        this.tasks = response.data[\"data\"];\n      }).catch(error => {\n        console.log(\"Error fetching tasks\", error);\n      });\n    },\n    updateTask(obj) {\n      update_task(obj.id, obj.value);\n      console.log(obj.done).then(response => {\n        console.log(\"msg\", response.data);\n        this.getTask();\n      }).catch(error => {\n        console.log(\"Erro ao atualizar task\", error);\n        this.getTask();\n      });\n    },\n    deleteTask(index) {\n      this.tasks.splice(index, 1);\n    },\n    deleteAll() {\n      this.tasks = [];\n    }\n  }\n};","map":{"version":3,"names":["get_task","update_task","InputGroup1","TodoItems","name","components","data","tasks","mounted","then","response","catch","error","console","log","methods","getTask","updateTask","obj","id","value","done","deleteTask","index","splice","deleteAll"],"sources":["/home/eduardo/Workspace/vue-todo-3/frontend/src/components/BaseComponent.vue"],"sourcesContent":["<template>\n    <div class=\"col-12\" @keyup.ctrl.delete=\"tasks=[]\">\n      <div class=\"card col-10 col-lg-3 col-md-6 col-sm-6\">\n        <div class=\"card-header\">\n          <h3><strong>TO-DO LIST</strong></h3>\n        </div>\n        <InputGroup1 />\n        <div class=\"card-body\">\n          <ul>\n          <TodoItems :items=\"tasks\" @delete-task=\"deleteTask\" @update-task=\"updateTask\"/>\n          \n        </ul>\n  \n        </div>\n       \n        <div class=\"card-footer\">\n          <div class=\"footer-content\">\n            <div class=\"itens-count\">\n              <a href=\"#\"> Tasks: {{ tasks.filter(i => i.checked == false).length  }}</a>\n  \n            </div>\n            <div class=\"delete-itens\">\n              <a href=\"#\" @click=\"deleteAll\"><i class=\"bi bi-trash3-fill\"></i></a>\n  \n            </div>\n            \n          </div>\n          \n        </div>\n        \n    </div>\n  \n    </div>\n  \n  </template>\n  \n  <script>\nimport { get_task, update_task } from '@/services/api';\nimport InputGroup1 from './input-group-1/InputGroup1.vue';\nimport TodoItems from './todoItems/TodoItems.vue';\n  \n  \n  export default {\n    name: 'BaseComponent',\n    components: {\n      InputGroup1,\n      TodoItems,\n    },\n    data(){\n      return{\n        tasks: []\n      }\n    }, \n    mounted(){\n      get_task()\n        .then((response) =>{\n          this.tasks = response.data[\"data\"];\n        })\n        .catch((error) =>{\n          console.log(\"Error fetching tasks\", error);\n        });\n    },\n    methods:{\n      getTask(){\n        get_task()\n        .then((response) =>{\n          this.tasks = response.data[\"data\"];\n        })\n        .catch((error) =>{\n          console.log(\"Error fetching tasks\", error);\n        });\n      },\n      updateTask(obj){\n        update_task(obj.id, obj.value)\n        console.log(obj.done)\n          .then((response) => {\n            console.log(\"msg\", response.data)\n            this.getTask();\n          }).catch((error) =>{\n            console.log(\"Erro ao atualizar task\",error);\n            this.getTask();\n          });\n      },\n      deleteTask(index){\n        this.tasks.splice(index, 1)\n      },\n      deleteAll(){\n        this.tasks = []\n  \n      }\n     \n    }\n  }\n  </script>\n  "],"mappings":"AAqCA,SAASA,QAAQ,EAAEC,WAAU,QAAS,gBAAgB;AACtD,OAAOC,WAAU,MAAO,iCAAi