{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { get_task } from '@/services/api';\nimport InputGroup1 from './input-group-1/InputGroup1.vue';\nimport TodoItems from './todoItems/TodoItems.vue';\nexport default {\n  name: 'BaseComponent',\n  components: {\n    InputGroup1,\n    TodoItems\n  },\n  data() {\n    return {\n      tasks: []\n    };\n  },\n  mounted() {\n    get_task().then(response => {\n      this.tasks = response.data[\"data\"];\n    }).catch(error => {\n      console.log(\"Error fetching tasks\", error);\n    });\n  },\n  methods: {\n    handleValue(value) {\n      if (value.trim() != \"\") this.tasks.push({\n        text: value,\n        checked: false\n      });\n    },\n    deleteTask(index) {\n      this.tasks.splice(index, 1);\n    },\n    deleteAll() {\n      this.tasks = [];\n    }\n  }\n};","map":{"version":3,"names":["get_task","InputGroup1","TodoItems","name","components","data","tasks","mounted","then","response","catch","error","console","log","methods","handleValue","value","trim","push","text","checked","deleteTask","index","splice","deleteAll"],"sources":["/home/eduardo/Workspace/vue-todo-3/frontend/src/components/BaseComponent.vue"],"sourcesContent":["<template>\n    <div class=\"col-12\" @keyup.ctrl.delete=\"tasks=[]\">\n      <div class=\"card col-10 col-lg-3 col-md-6 col-sm-6\">\n        <div class=\"card-header\">\n          <h3><strong>TO-DO LIST</strong></h3>\n        </div>\n        <InputGroup1 @data-item =\"handleValue\"/>\n        <div class=\"card-body\">\n          <ul>\n          <TodoItems :items=\"tasks\" @delete-task=\"deleteTask\"/>\n          \n        </ul>\n  \n        </div>\n       \n        <div class=\"card-footer\">\n          <div class=\"footer-content\">\n            <div class=\"itens-count\">\n              <a href=\"#\"> Tasks: {{ tasks.filter(i => i.checked == false).length  }}</a>\n  \n            </div>\n            <div class=\"delete-itens\">\n              <a href=\"#\" @click=\"deleteAll\"><i class=\"bi bi-trash3-fill\"></i></a>\n  \n            </div>\n            \n          </div>\n          \n        </div>\n        \n    </div>\n  \n    </div>\n  \n  </template>\n  \n  <script>\nimport { get_task } from '@/services/api';\nimport InputGroup1 from './input-group-1/InputGroup1.vue';\nimport TodoItems from './todoItems/TodoItems.vue';\n  \n  \n  export default {\n    name: 'BaseComponent',\n    components: {\n      InputGroup1,\n      TodoItems,\n    },\n    data(){\n      return{\n        tasks: []\n      }\n    }, \n    mounted(){\n      get_task()\n        .then((response) =>{\n          this.tasks = response.data[\"data\"];\n        })\n        .catch((error) =>{\n          console.log(\"Error fetching tasks\", error);\n        })\n    },\n    methods:{\n      handleValue(value){\n        if(value.trim() != \"\")\n        this.tasks.push({text:value, checked:false})\n      },\n      deleteTask(index){\n        this.tasks.splice(index, 1)\n  \n      },\n      deleteAll(){\n        this.tasks = []\n  \n      }\n     \n    }\n  }\n  </script>\n  "],"mappings":";AAqCA,SAASA,QAAO,QAAS,gBAAgB;AACzC,OAAOC,WAAU,MAAO,iCAAiC;AACzD,OAAOC,SAAQ,MAAO,2BAA2B;AAG/C,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVH,WAAW;IACXC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,KAAK,EAAE;IACT;EACF,CAAC;EACDC,OAAOA,CAAA,EAAE;IACPP,QAAQ,CAAC,EACNQ,IAAI,CAAEC,QAAQ,IAAI;MACjB,IAAI,CAACH,KAAI,GAAIG,QAAQ,CAACJ,IAAI,CAAC,MAAM,CAAC;IACpC,CAAC,EACAK,KAAK,CAAEC,KAAK,IAAI;MACfC,OAAO,CAACC,GAAG,CAAC,sBAAsB,EAAEF,KAAK,CAAC;IAC5C,CAAC;EACL,CAAC;EACDG,OAAO,EAAC;IACNC,WAAWA,CAACC,KAAK,EAAC;MAChB,IAAGA,KAAK,CAACC,IAAI,CAAC,KAAK,EAAE,EACrB,IAAI,CAACX,KAAK,CAACY,IAAI,CAAC;QAACC,IAAI,EAACH,KAAK;QAAEI,OAAO,EAAC;MAAK,CAAC;IAC7C,CAAC;IACDC,UAAUA,CAACC,KAAK,EAAC;MACf,IAAI,CAAChB,KAAK,CAACiB,MAAM,CAACD,KAAK,EAAE,CAAC;IAE5B,CAAC;IACDE,SAASA,CAAA,EAAE;MACT,IAAI,CAAClB,KAAI,GAAI,EAAC;IAEhB;EAEF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}